// MARK: Dependency extensions for Injected types
{% for type in types.implementing.Injected %}
{% if type.kind == "protocol" and type.implements.count == 1 %}
extension Dependency where T:{{type.name}} {
    {% for var in type.allVariables %}
    var {{var.name}}: {{var.typeName.name}} { dependency.{{var.name}} }
    {% endfor %}
}
{% endif %}
{% endfor %}
